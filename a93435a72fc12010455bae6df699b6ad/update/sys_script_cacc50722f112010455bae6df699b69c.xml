        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var acccessToken = getToken();
	var epId;
	var networkId = '7c57fdae-104a-4026-8d2d-1c69524895e7';
	if (accessToken) {
		var epName = current.u_requested_for.last_name + "." + current.u_requested_for.first_name + "." + current.u_which_appwan;
		epId = createEndpoint(accessToken, networkId, epName, current.u_which_appwan);
		if (epId) {
			gs.info("created endpoint: "+ epName + " with ID: "+ epId);
			shareEndpointViaEmail(accessToken, epName, epId, current.u_requested_for.email);
		} else {
			gs.error("endpoint creation failed: "+ epName);
		}
	} else {
		gs.error('Failed to get Cognito Access Token for secure access request for ' + current.u_requested_for.name);
	}
			
})(current, previous);]]></script>
